{
  "name": "not-twitter",
  "version": "1.0.6",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build:express": "tsc --project ./tsconfig.express.json",
    "dev:express": "tsc-watch --onSuccess \"node backend/dist/src/api.js\" -p tsconfig.express.json",
    "prod:express": "node /backend/dist/src/api.js",
    "test:unit": "vitest --environment jsdom --root src/ ",
    "test:unit:single": "vitest run --environment jsdom --root src",
    "test:unit:backend": "vitest --environment jsdom --root backend/tests/",
    "test:unit:backend:single": "vitest run --environment jsdom --root backend/tests/",
    "test:unit:coverage": "vitest --environment jsdom --root src/ --coverage",
    "test:unit:coverage:single": "vitest run --environment jsdom --root src/ --coverage",
    "test:unit:backend:coverage": "vitest --environment jsdom --root backend/ --coverage",
    "test:unit:backend:coverage:single": "vitest run --environment jsdom --root backend/ --coverage",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "ci:coverage": "vitest --environment jsdom --coverage --config ./vite.config.ts",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^6.7.2",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5",
    "vuetify": "^3.0.3"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.68",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vitest/coverage-c8": "^0.26.2",
    "@vitest/coverage-istanbul": "^0.25.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^10.10.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "madge": "^5.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-vuetify": "^1.0.1",
    "vitest": "^0.24.3",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^1.0.12"
  }
}
